{
  "name": "michel-world",
  "version": "1.2.1",
  "description": "React version of Argon Dashboard by Creative Tim",
  "main": "index.js",
  "keywords": [
    "react",
    "reactjs",
    "michel-world"
  ],
  "author": "Micheo Nguyen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/creativetimofficial/michel-world/issues"
  },
  "homepage": "https://demos.creative-tim.com/michel-world/",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/michel-world.scss src/assets/css/michel-world.css",
    "minify:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/michel-world.scss src/assets/css/michel-world.min.css --output-style compressed",
    "map:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/michel-world.scss src/assets/css/michel-world.css --source-map true",
    "build:scss": "npm run compile:scss && npm run minify:scss && npm run map:scss"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.3",
    "@reduxjs/toolkit": "^1.7.1",
    "axios": "^0.24.0",
    "bootstrap": "4.6.0",
    "chart.js": "2.9.4",
    "classnames": "2.3.1",
    "express": "^4.17.1",
    "html-react-parser": "^1.4.5",
    "jwt-decode": "^3.1.2",
    "millify": "^4.0.0",
    "moment": "2.29.1",
    "node-sass": "6.0.1",
    "node-sass-package-importer": "5.3.2",
    "nouislider": "15.2.0",
    "react": "17.0.2",
    "react-bootstrap-typeahead": "^6.0.0-alpha.6",
    "react-chartjs-2": "^2.11.1",
    "react-copy-to-clipboard": "5.0.3",
    "react-datetime": "3.0.4",
    "react-dom": "17.0.2",
    "react-google-login": "^5.2.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "5.2.0",
    "react-scripts": "^4.0.3",
    "react-typical": "^0.1.3",
    "reactstrap": "8.9.0",
    "styled-components": "^5.3.1",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.9"
  },
  "optionalDependencies": {
    "eslint-plugin-flowtype": "5.8.0",
    "jquery": "3.6.0",
    "typescript": "4.3.5"
  }
}
